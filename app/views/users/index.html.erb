<% provide(:title, "All Users") %>

<h1>User index</h1>
<p>temp : Sign in</p>

<%= will_paginate %>
<div>
    <h2>All Users</h2>
    <ul> 
        <% @users.each do |user| %>
            <li><%= user.name %> :: <%= user.email %></li>
            <% if current_user.admin? && !current_user?(user) %>
                <%= link_to "delete", user, method: :delete %>
            <% end %>
        <% end %>
    </ul>
</div>
<%= will_paginate %>
